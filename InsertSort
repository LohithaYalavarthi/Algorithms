function insertionSort(arr, n) 
{ 
   var i, key, j; 
   for (i = 1; i < n; i++) 
   { 
       key = arr[i]; 
       j = i-1; 
  
       
       while (j >= 0 && arr[j] > key) 
       { 
           arr[j+1] = arr[j]; 
           j = j-1; 
       } 
       arr[j+1] = key; 
   } 
  console.log(arr);

} 

    var arr = [12, 11, 13, 5, 6]; 
    var n = arr.length;

    insertionSort(arr,n)
