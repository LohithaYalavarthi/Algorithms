getlengthforArrays(arr1, arr2, lenArr1, lenArr2){
	var arr1= arr1.length;
    var arr2 = arr2.length;
    var smllen ;
    if(arr1 > arr2){
     smllen = arr2.length
     whicharr= arr2
    }
    else{
     smllen = arr1.length
     whicharr= arr1
     }
     return [smllen,whicharr]
}

